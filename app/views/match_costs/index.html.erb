<h1>Listing match_costs</h1>

<table>
  <thead>
    <tr>
      <th>Cost code</th>
      <th>Mid</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @match_costs.each do |match_cost| %>
      <tr>
        <td><%= match_cost.cost_code %></td>
        <td><%= match_cost.mID %></td>
        <td><%= link_to 'Show', match_cost %></td>
        <td><%= link_to 'Edit', edit_match_cost_path(match_cost) %></td>
        <td><%= link_to 'Destroy', match_cost, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Match cost', new_match_cost_path %>
